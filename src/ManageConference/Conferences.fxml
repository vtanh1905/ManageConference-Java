<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="615.0" prefWidth="830.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ManageConference.ConferencesController">
   <children>
      <BorderPane fx:id="bpaneContent" prefHeight="615.0" prefWidth="830.0">
         <top>
            <HBox alignment="CENTER" prefHeight="27.0" prefWidth="830.0" style="-fx-background-color: #EBE8F9;" BorderPane.alignment="CENTER">
               <children>
                  <ImageView fitHeight="26.0" fitWidth="22.0" pickOnBounds="true" preserveRatio="true">
                     <HBox.margin>
                        <Insets right="15.0" />
                     </HBox.margin>
                     <image>
                        <Image url="@../images/icons8_Search_52px.png" />
                     </image>
                  </ImageView>
                  <TextField fx:id="txtTimKiem" prefHeight="25.0" prefWidth="325.0" />
                  <ImageView fx:id="imgGrid" fitHeight="26.0" fitWidth="22.0" pickOnBounds="true">
                     <image>
                        <Image url="@../images/icons8-grid-view-26-active.png" />
                     </image>
                     <HBox.margin>
                        <Insets left="15.0" right="15.0" />
                     </HBox.margin>
                  </ImageView>
                  <ImageView fx:id="imgList" fitHeight="26.0" fitWidth="22.0" layoutX="595.0" layoutY="15.0" pickOnBounds="true">
                     <image>
                        <Image url="@../images/icons8-bulleted-list-50.png" />
                     </image>
                  </ImageView>
               </children>
               <padding>
                  <Insets bottom="5.0" top="5.0" />
               </padding>
            </HBox>
         </top>
         <bottom>
            <HBox alignment="CENTER" prefHeight="15.0" prefWidth="830.0" style="-fx-background-color: #EBE8F9;" BorderPane.alignment="BOTTOM_CENTER">
               <children>
                  <Button fx:id="btnLeft" mnemonicParsing="false" onAction="#onAction_btnLeft" text="&lt;" />
                  <TextField fx:id="txtPage" prefHeight="25.0" prefWidth="42.0" text="1/10">
                     <HBox.margin>
                        <Insets left="5.0" right="5.0" />
                     </HBox.margin>
                  </TextField>
                  <Button fx:id="btnRight" mnemonicParsing="false" onAction="#onAction_btnRight" text="&gt;" />
               </children>
               <padding>
                  <Insets bottom="7.0" top="3.0" />
               </padding></HBox>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
