<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="30.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" layoutY="3.0" prefHeight="35.0" prefWidth="800.0" style="-fx-border-width: 0 0 2 0; -fx-border-color: #FFFFFF;">
         <children>
            <Label fx:id="lblID" alignment="CENTER" prefHeight="28.0" prefWidth="56.0" text="ID">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="lblHoTen" alignment="CENTER" prefHeight="36.0" prefWidth="224.0" text="Họ tên">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="lblTaiKhoan" alignment="CENTER" prefHeight="36.0" prefWidth="234.0" text="Tài khoản">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="lblEmail" alignment="CENTER" prefHeight="36.0" prefWidth="184.0" text="Email">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Button fx:id="btnTrangThai" alignment="CENTER" mnemonicParsing="false" onAction="#onAction_btnTrangThai" prefHeight="9.0" prefWidth="92.0" style="-fx-background-color: #EBE8F9; -fx-border-color: #2A73FF; -fx-border-radius: 30;" text="Không chặn" />
         </children>
      </HBox>
   </children>
</AnchorPane>
